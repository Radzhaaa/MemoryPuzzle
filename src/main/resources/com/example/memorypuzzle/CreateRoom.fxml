<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.memorypuzzle.CreateRoomController">
   <children>
      <VBox  layoutX="220.0" layoutY="122.0">
         <children>
            <Label prefHeight="37.0" prefWidth="176.0" text="Room name:" />
            <TextField fx:id="roomName" prefHeight="20.0" prefWidth="176.0" />
         </children>
      </VBox>
      <Button fx:id="createRoomButton" layoutX="202.0" layoutY="278.0" mnemonicParsing="false" onAction="#CreateRoom" text="CreateRoom" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackView" prefHeight="13.0" prefWidth="70.0" style="-fx-background-color: black; -fx-background-radius: 5px; -fx-font-size: 15px;" text="back" textFill="WHITE">
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </Button>
   </children>
</AnchorPane>
