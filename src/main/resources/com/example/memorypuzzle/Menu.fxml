<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.memorypuzzle.MenuController">
   <children>
      <Label layoutX="83.0" layoutY="54.0" prefWidth="433.0" text="Welcome to the Memory Puzzle!" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Zapf Dingbats" size="13.0" />
         </font>
      </Label>
      <Button fx:id="button1" layoutX="88.0" layoutY="181.0" mnemonicParsing="false" onAction="#OnePlayer" prefHeight="60.0" prefWidth="168.0" text="1 player" />
      <Button fx:id="button2" layoutX="344.0" layoutY="181.0" mnemonicParsing="false" onAction="#TwoPlayers" prefHeight="60.0" prefWidth="168.0" text="2 players" />
   </children>
</AnchorPane>
